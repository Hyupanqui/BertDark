{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "types": [
      "astro/client"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/lib/*": [
        "src/lib/*"
      ],
      "@/icons/*": [
        "src/components/icons/*"
      ],
      "@/layouts/*": [
        "src/layouts/*"
      ],
      "@/i18n/*": [
        "src/i18n/*"
      ],
    }
  }
}
